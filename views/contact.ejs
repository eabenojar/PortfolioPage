<% include partials/header %>


<div class="container-fluid">
  <section class="contact-main">
    <div class="contact-section">
      <div class="container">
        <h1 class="contact-heading">Get <strong class="contact-heading-strong">in touch</strong</h1>
        <h2 class="contact-title">I am currently available for freelance work and projects.</h2>
        <p class="contact-description">If you are interested in working with me feel free to get in touch.</p>
        <form method="POST" action="/contact">
          <div class="form-group">
              <input type="email" placeholder="Name" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
            </div>
            <div class="form-group">
              <input type="text" placeholder="E-mail"class="form-control" id="exampleInputPassword1" placeholder="Password">
            </div>
            <div class="form-group">
              <textarea class="form-control" maxlength="300" id="exampleFormControlTextarea1" placeholder="Write a message"rows="5"></textarea>
            </div>
        </form>
        <button class="contact-button">CONTACT ME</button>
    </div>
  </div>
</section>
</div>


<% include partials/footer %>
